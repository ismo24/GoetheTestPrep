{
  "expo": {
    "name": "GoetheTestPrep",
    "slug": "GoetheTestPrep",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.goethetestprep",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "google-auth",
            "CFBundleURLSchemes": ["com.yourcompany.goethetestprep"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.goethetestprep"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-av",
      "expo-web-browser",
      "expo-apple-authentication"
    ],
    "scheme": "goethetestprep"
  }
}